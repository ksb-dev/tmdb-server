    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.3.0",
    "helmet": "^5.0.2",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.9",
    "rate-limiter": "^0.2.0",
    "react-dotenv": "^0.1.3",
    "react-slideshow-image": "^4.2.0",
    "react-youtube-player": "^2.0.1",
    "swagger-ui-express": "^4.3.0",
    "xss-clean": "^0.1.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
